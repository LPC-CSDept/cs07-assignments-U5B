    "isspace = lambda input: input == ' '\n",
    "def getalnum(msg):\n",
    "  stripString = ''\n",
    "  for char in msg:\n",
    "    if (isspace(char) == False):\n",
    "      stripString += char\n",
    "  yield stripString\n",
    "  \n",
    "res = getalnum('Python programming section 2')\n",
    "for v in res:\n",
    "  print(v)\n"
   ]
  }
