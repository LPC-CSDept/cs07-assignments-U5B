    "def sumProduct(list1, list2):\n",
    "  sumList = []\n",
    "  sum = 0\n",
    "  for i in range(len(list1)):\n",
    "    sumList.append(list1[i] * list2[i])\n",
    "  for i in range(len(list1)):\n",
    "    sum += sumList[i]\n",
    "  return sum\n",
    "\n",
    "result = sumProduct([2, 2, 2], [2, 2, 2])\n",
    "print(result)"
   ]
  }
